class Human:
    def __init__(self, first_name, second_name):
        self.first_name = first_name
        self.second_name = second_name
 
 
class Teacher:
    def __init__(self, intelligence, competence):
        self.intelligence = intelligence
        self.competence = competence
 
 
class DreamTeacher(Human, Teacher):
    def __init__(self, first_name, second_name, intelligence, competence, empathy, charm):
        Human.__init__(self, first_name, second_name)
        Teacher.__init__(self, intelligence, competence)
        self.empathy = empathy
        self.charm = charm
 
    def __str__(self):
